{
    "name": "dantleech/php-diagram",
    "description": "Generate DOT Diagrams from a DSL",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "symfony/console": "^5.1",
        "mathiasverraes/parsica": "^0.5.0",
        "thecodingmachine/safe": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.42",
        "phpunit/phpunit": "^9.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "DTL\\PhpDiagram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\PhpDiagram\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
